{
    "name": "tharindu/laravel-permission-ui",
    "description": "A Laravel package to manage roles and permissions with UI implementation.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/config": "^10.0|^11.0",
        "spatie/laravel-permission": "^6.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tharindu\\LaravelPermissionUI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tharindu\\LaravelPermissionUI\\PermissionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
